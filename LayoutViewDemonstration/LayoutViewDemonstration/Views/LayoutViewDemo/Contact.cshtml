@{
    ViewData["Title"] = "Contact us";
    ViewData["Temp"] = "Temp Data";

    var myViewDictionary = new ViewDataDictionary(ViewData);
    myViewDictionary["Title"] = "Partial View + Contact us";
    myViewDictionary["Temp"] = "Updated Temp data";

}
<h1>Contact Us</h1>

@section footer_section {
    <p>Contact us: 123457890</p>
}

<h2>
    Using partial Tag helper
</h2>
<partial name="_DemoPartialView" view-data="myViewDictionary" />
<h2>Strongly Typed View</h2>
<h2>Using Html.PartialAsync</h2>
@await Html.PartialAsync("_DemoPartialView")
<h2>Using Html.RenderPartialAsync - always inside a razor block</h2>
@{
    await Html.RenderPartialAsync("_DemoPartialView");
}



